{
  "schemaVersion": 1,
  "id": "chunksfadein",
  "version": "${version}",
  "name": "Chunks Fade In",
  "description": "Adds bedrock-like chunk fade-in animation and chunk animations like in ChunkAnimator mod!",
  "authors": [
    "Koteinik"
  ],
  "contact": {
    "homepage": "https://github.com/kerudion/chunksfadein",
    "sources": "https://github.com/kerudion/chunksfadein",
    "issues": "https://github.com/kerudion/chunksfadein/issues"
  },
  "license": "GPLv3",
  "icon": "assets/chunksfadein/icon.png",
  "environment": "client",
  "mixins": [
    "chunksfadein.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.7",
    "minecraft": "~1.21",
    "java": ">=21",
    "sodium": ">=0.5.20"
  },
  "breaks": {
    "iris": "<1.7.20",
    "optifabric": "*"
  },
  "entrypoints": {
    "client": [
      "com.koteinik.chunksfadein.ChunksFadeIn"
